    "license": {
      "$id": "/properties/license",
      "type": "string",
      "title": "The License Schema ",
      "default": "",
      "examples": [
        "gpl3"
      ]
    },
    "categories": {
      "$id": "/properties/categories",
      "type": "array",
      "items": {
        "$id": "/properties/categories/items",
        "type": "string",
        "title": "The 0th Schema ",
        "default": "",
        "examples": [
          "Development"
        ]
      }
    },
    "keywords": {
      "$id": "/properties/keywords",
      "type": "array",
      "items": {
        "$id": "/properties/keywords/items",
        "type": "string",
        "title": "The 0th Schema ",
        "default": "",
        "examples": [
          "application",
          "application",
          "example",
          "ejemplo"
        ]
      }
    },
    "screenshots": {
      "$id": "/properties/screenshots",
      "type": "array",
      "items": {
        "$id": "/properties/screenshots/items",
        "type": "string",
        "title": "The 0th Schema ",
        "default": "",
        "examples": [
          ""
        ]
      }
    },
    "icon": {
      "$id": "/properties/icon",
      "type": "array",
      "items": {
        "$id": "/properties/icon/items",
        "type": "string",
        "title": "The 0th Schema ",
        "default": "",
        "examples": [
          ""
        ]
      }
