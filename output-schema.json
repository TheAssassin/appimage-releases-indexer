    "releases": {
      "$id": "/properties/releases",
      "type": "array",
      "items": {
        "$id": "/properties/releases/items",
        "type": "object",
        "properties": {
          "version": {
            "$id": "/properties/releases/items/properties/version",
            "type": "string",
            "title": "The Version Schema ",
            "default": "",
            "examples": [
              "0.1.0"
            ]
          },
          "release_date": {
            "$id": "/properties/releases/items/properties/release_date",
            "type": "string",
            "title": "The Release_date Schema ",
            "default": "",
            "examples": [
              "10/10/2017"
            ]
          },
          "changelog": {
            "$id": "/properties/releases/items/properties/changelog",
            "type": "object",
            "properties": {
              "en": {
                "$id": "/properties/releases/items/properties/changelog/properties/en",
                "type": "string",
                "title": "The En Schema ",
                "default": "",
                "examples": [
                  "<p>The application was created.</p>"
                ]
              },
              "es": {
                "$id": "/properties/releases/items/properties/changelog/properties/es",
                "type": "string",
                "title": "The Es Schema ",
                "default": "",
                "examples": [
                  "<p>La applicaci'on fue creada.</p>"
                ]
              }
            }
          },
          "files": {
            "$id": "/properties/releases/items/properties/files",
            "type": "array",
            "items": {
              "$id": "/properties/releases/items/properties/files/items",
              "type": "object",
              "properties": {
                "url": {
                  "$id": "/properties/releases/items/properties/files/items/properties/url",
                  "type": "string",
                  "title": "The Url Schema ",
                  "default": "",
                  "examples": [
                    "https://github.com/AppImage/AppImageKit/releases/download/continuous/appimagetool-x86_64.AppImage"
                  ]
                },
                "architecture": {
                  "$id": "/properties/releases/items/properties/files/items/properties/architecture",
                  "type": "string",
                  "title": "The Architecture Schema ",
                  "default": "",
                  "examples": [
                    "x86-64"
                  ]
                },
                "sha512checksum": {
                  "$id": "/properties/releases/items/properties/files/items/properties/sha512checksum",
                  "type": "string",
                  "title": "The Sha512checksum Schema ",
                  "default": "",
                  "examples": [
                    "a big and fussy checksum"
                  ]
                },
                "size": {
                  "$id": "/properties/releases/items/properties/files/items/properties/size",
                  "type": "integer",
                  "title": "The Size Schema ",
                  "default": 0,
                  "examples": [
                    1882040
                  ]
                }
              }
            }
          }
        }
      }
