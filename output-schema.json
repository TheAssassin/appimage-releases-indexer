        "example-app.desktop"
      ]
    },
    "name": {
      "$id": "/properties/name",
      "type": "object",
      "properties": {
        "en": {
          "$id": "/properties/name/properties/en",
          "type": "string",
          "title": "The En Schema ",
          "default": "",
          "examples": [
            "Example App"
          ]
        },
        "es": {
          "$id": "/properties/name/properties/es",
          "type": "string",
          "title": "The Es Schema ",
          "default": "",
          "examples": [
            "Aplicacion de ejemplo"
          ]
        }
      }
    },
    "abstract": {
      "$id": "/properties/abstract",
      "type": "object",
      "properties": {
        "en": {
          "$id": "/properties/abstract/properties/en",
          "type": "string",
          "title": "The En Schema ",
          "default": "",
          "examples": [
            "Domostrative Application."
          ]
        },
        "es": {
          "$id": "/properties/abstract/properties/es",
          "type": "string",
          "title": "The Es Schema ",
          "default": "",
          "examples": [
            "Applicaci'on demostrativa."
          ]
        }
      }
    },
    "description": {
      "$id": "/properties/description",
      "type": "object",
      "properties": {
        "en": {
          "$id": "/properties/description/properties/en",
          "type": "string",
          "title": "The En Schema ",
          "default": "",
          "examples": [
            "<p>English long description of example app</p>"
          ]
        },
        "es": {
          "$id": "/properties/description/properties/es",
          "type": "string",
          "title": "The Es Schema ",
          "default": "",
          "examples": [
            "<p>Descripcion en Espa~nol de la Aplicaci@oacuten de Ejemplo</p>"
          ]
        }
      }
